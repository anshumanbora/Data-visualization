{
  "root": true,
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
  "browser": true
  },
  "rules": {
    "no-unused-expressions": "off",
    "no-plusplus": "off",
    "import/no-extraneous-dependencies": "off",
    "react/forbid-prop-types": "off",
    "react/jsx-filename-extension": "off",
    "react/no-array-index-key": "off",
    "linebreak-style": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": false
    }
  },
  "plugins": [
    "react"
  ]
}
